package drools.templates;

import bsep.sc.SiemCenter.model.TestMessage;

template "test template"

rule "test rule @{row.rowNumber}"
    no-loop true
    when
        $msg: TestMessage($val: value, @{attribute} @{comparator} "@{value}")

    then
        System.out.println("Matching message " + $val);
end

end template